CREATE TABLE users(
id INTEGER AUTO_INCREMENT PRIMARY KEY,
nazwa VARCHAR(40) UNIQUE,
haslo VARCHAR(40),
email VARCHAR(40),
rejestracja INTEGER,
logowanie INTEGER,
ip VARCHAR(40)
);

